import nltk

nltk.download('punkt')

grammar = nltk.CFG.fromstring("""
S -> NP VP
NP -> Det N
Det -> 'the'
N -> 'cat' | 'mouse'
VP -> V NP
V -> 'chased'
""")

parser = nltk.ChartParser(grammar)

sentence = ["the", "cat", "chased", "the", "mouse"]

for tree in parser.parse(sentence):
    tree.pretty_print()
